amazon==azoamn

#include<stdlib.h>
#include<stdio.h>
#include<string.h>
int main(){
	char a[]="triangle";
	char b[]="integral";
	int count=0;
	if(strlen(a)==strlen(b)){
		for(int i=0;i<strlen(a);i++){
			for(int j=0;j<strlen(b);j++){
				if(a[i]==b[j]){
					b[j]='0';
					count++;
					break;
				}
			}
		}
	if(count==strlen(a)){
		printf("YES");
		return 0;
	}
	}
	printf("NO");
	

}
