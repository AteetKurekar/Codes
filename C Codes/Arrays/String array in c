#include<stdlib.h>
#include<stdio.h>
#include<string.h>

int main(){
	char a[10][100]={'\0'};
	char s[]="Ateet Kurekar";
	int row=0,col=0;
	
	for(int i=0;i<strlen(s);i++){
		if(s[i]==' '){
			row++;
			col=0;
			continue;
		}
		a[row][col]=s[i];
		col++;	
	}
	printf("%s\n",a[1]);
//	printf("%d\n",strcmp(a[1],"Kurekar"));
//	char b[100];
//	strcpy(b,a[1]);
//	printf("%s\n",b);
//	printf("%d",strlen(a[1]));
	
	
}

  
