#include<stdlib.h>
#include<stdio.h>
#include<string.h>
int main(){
	char a[]="Atexyzetsdf";
	for(int i=0;i<strlen(a)-1;i++){
		for(int j=i+1;j<strlen(a);j++){
			if(a[i]==a[j]){
				a[j]='0';
			}
		}
	}
for(int i=0;i<strlen(a);i++){
	if(a[i]!='0')
		printf("%c",a[i]);
}

}
other solution:Hashmaps
